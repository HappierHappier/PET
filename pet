local a=loadstring(game:HttpGet('https://sirius.menu/rayfield'))()local b=a:CreateWindow({Name="Main",Icon=0,LoadingTitle="Rayfield Interface Suite",LoadingSubtitle="by Sirius",Theme="Default",DisableRayfieldPrompts=false,DisableBuildWarnings=false,ConfigurationSaving={Enabled=true,FileName="Big Hub"}})local c=b:CreateTab("Main",4483362458)local d=b:CreateTab("Obby",4483362458)local e=false;local f=false;local g=c:CreateToggle({Name="Auto Farm",CurrentValue=false,Flag="AutoFarmToggle",Callback=function(h)e=h end})local i=c:CreateToggle({Name="Free Money",CurrentValue=false,Flag="FreeMoneyToggle",Callback=function(h)f=h end})local j=c:CreateButton({Name="Inf Spins",Callback=function()local k={[1]="Spin",[2]={["Image"]="rbxassetid://112293371233863",["Rotations"]=135,["Amount"]=200000,["Chance"]=0.2,["RewardName"]="Spin",["DisplayName"]="x3 Spins"}}game:GetService("ReplicatedStorage").Network.RewardSpin:FireServer(unpack(k))end})local l=c:CreateButton({Name="Claim All Gifts",Callback=function()for m=1,12 do local k={[1]="Gift"..m}game:GetService("ReplicatedStorage").Network.ClaimGift:InvokeServer(unpack(k))end end})local n=d:CreateButton({Name="Finish Ice Obby",Callback=function()local k={[1]="End",[2]="IceObby"}game:GetService("ReplicatedStorage").Network.StartMinigameTime:FireServer(unpack(k))end})spawn(function()while true do wait(0.001)if e then game:GetService("ReplicatedStorage").Network.TakeDamage:InvokeServer()end end end)spawn(function()while true do wait(1)if f then local k={[1]="+"}game:GetService("ReplicatedStorage").Network.GetValue:FireServer(unpack(k))wait(0.001)local o={[1]="Coins",[2]={["Image"]="rbxassetid://101706852515688",["Rotations"]=225,["Amount"]=5,["Chance"]=0.5,["DisplayName"]="+5% Coins",["RewardName"]="Coins"}}game:GetService("ReplicatedStorage").Network.RewardSpin:FireServer(unpack(o))end end end)
